{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "outputDocument",
      "type": "cosmosDB",
      "databaseName": "ratings",
      "collectionName": "ratings",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOSDBACCOUNTCONNECTIONSTRING",
      "direction": "out",
      "partitionKey": "id",
      "collectionThroughput": "1000"
  }
  ]
}
